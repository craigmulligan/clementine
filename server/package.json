{
  "name": "clementine",
  "private": true,
  "version": "1.0.0",
  "description": "in-progress",
  "main": "config.js",
  "scripts": {
    "wait:db": "wait-on tcp:postgres:5432 && echo 'postgres is up!'",
    "test": "npm run wait:db && jest --watchAll",
    "ci": "npm run wait:db && npm test",
    "format": "prettier",
    "start": "node ./bin/start.js",
    "dev": "nodemon ./bin/start.js",
    "migrate": "npm run wait:db && node ./bin/migrate.js up",
    "migrate:down": "npm run wait:db && node ./bin/migrate.js down",
    "migrate:create": "npm run wait:db && migrate create --migrations-dir='./src/migrations'"
  },
  "keywords": ["graphql"],
  "author": "hobochild",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.11.0",
    "bcrypt": "^4.0.1",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^14.6.0",
    "graphql-auth": "^0.2.6",
    "graphql-scalars": "^1.0.9",
    "helmet": "^3.16.0",
    "migrate": "^1.6.2",
    "morgan": "^1.9.1",
    "pg": "^7.9.0",
    "pg-format": "^1.0.4",
    "redis": "^3.0.2",
    "sql-template-strings": "^2.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "wait-on": "^4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hobochild/clementine.git"
  },
  "bugs": {
    "url": "https://github.com/hobochild/clementine/issues"
  },
  "homepage": "https://github.com/hobochild/clementine#readme"
}
